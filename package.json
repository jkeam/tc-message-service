{
  "name": "tc-message-service",
  "version": "1.1.1",
  "description": "Topcoder platform messaging service",
  "main": "app/index.js",
  "engines": {
    "node": ">=6.9"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/appirio-tech/tc-message-service.git"
  },
  "keywords": [
    "Topcoder",
    "messaging"
  ],
  "author": "Mauricio Desiderio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/appirio-tech/tc-message-service/issues"
  },
  "homepage": "https://github.com/appirio-tech/tc-message-service#readme",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint . --fix || true",
    "build": "babel app -d dist --presets es2015",
    "prestart": "npm run -s build",
    "start": "node dist",
    "start:dev": "NODE_ENV=local PORT=8001 nodemon -w app --exec \"babel-node app --presets es2015\" | ./node_modules/.bin/bunyan",
    "xstart:dev": "NODE_ENV=local nodemon server.js | ./node_modules/.bin/bunyan",
    "test": "npm run lint && LOG_LEVEL=debug NODE_ENV=test TC_MESSAGE_SERVICE_AUTH_LOOSE=true ./node_modules/.bin/istanbul cover -x '*.spec.js' ./node_modules/.bin/_mocha -- --compilers js:babel-core/register $(find app -name '*.spec.js')",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/mocha -w --compilers js:babel-core/register $(find app -name '*.spec.js')"
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "aws-sdk": "^2.200.0",
    "axios": "^0.16.1",
    "babel-eslint": "^8.2.1",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "common-errors": "^1.0.0",
    "config": "^1.21.0",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.3",
    "digest-stream": "^2.0.0",
    "express": "^4.14.0",
    "express-list-routes": "^0.1.4",
    "express-request-id": "^1.1.0",
    "express-validation": "^1.0.0",
    "form-data": "^2.2.0",
    "joi": "^8.0.5",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.13.1",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "pg": "^6.0.2",
    "pg-hstore": "^2.3.2",
    "querystring": "^0.2.0",
    "sequelize": "^4.13.5",
    "tc-core-library-js": "git+https://github.com/appirio-tech/tc-core-library-js.git#v2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "concat-stream": "^1.6.0",
    "csv-parse": "^1.1.8",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.1",
    "sequelize-cli": "^2.4.0",
    "should": "^10.0.0",
    "should-sinon": "0.0.5",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "supertest": "^1.2.0"
  }
}
